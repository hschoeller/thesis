{
    "latex-workshop.view.pdf.internal.synctex.keybinding": "double-click",
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.latex.outDir": "build",
    "latex-workshop.view.pdf.refresher": "auto",
    "latex-workshop.intellisense.enabled": true,
    "latex-workshop.intellisense.ref.enabled": true,
    "latex-workshop.intellisense.citation.enabled": true,
    "latex-workshop.intellisense.label.enabled": true,
    "latex-workshop.latex.recipes": [
        {
            "name": "Build with Makefile",
            "tools": [
                "makefile"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "makefile",
            "command": "make",
            "args": [
                "all"
            ]
        }
    ],
    "latex-workshop.latex.defaultRecipe": "Build with Makefile"
}